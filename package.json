{
  "name": "visuino-component-creator",
  "displayName": "Visuino Component Creator",
  "description": "Create Structure for Visuino Component",
  "author": {
    "name": "Swellington Soares",
    "url": "https://github.com/Swellington-Soares"
  },
  "publisher": "SwellingtonSoares",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#f2c46f",
    "theme": "dark"
  },
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swellington-Soares/visuino-component-creator.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "visuino-component-creator.createComponent",
        "title": "Create Visuino Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "visuino-component-creator.createComponent",
          "when": "explorerResourceIsFolder",
          "group": "navigator"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "install": "^0.13.0",
    "npm": "^11.4.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  }
}
